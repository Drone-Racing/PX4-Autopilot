#!/bin/sh
#
# KakuteH7 specific board extras init
#------------------------------------------------------------------------------

if ! param compare OSD_ATXXXX_CFG 0
then
	atxxxx start -s
fi

# DShot telemetry is always on UART7
dshot telemetry /dev/ttyS5

sleep 30
microdds_client start -t serial -d /dev/ttyS3 -b 3000000
